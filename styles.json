{
    "pageSetup" : {
        "size": "letter",
        "firstTemplate": "curriculum",
        "margin-top": "1.5cm",
        "margin-bottom": "1.5cm",
        "margin-left": "1.5cm",
        "margin-right": "1.5cm",
        "margin-gutter": "0cm",
        "spacing-header": "5mm",
        "spacing-footer": "5mm"

    },
    "pageTemplates": {
        "curriculum": {
            "frames": [
                ["0cm", "83%", "100%", "19%"],
                ["0cm", "0cm", "50%", "85%"],
                ["51%", "0cm", "50%", "85%"]
            ],
        "showHeader" : "true",
        "showFooter" : "true"
        }
    },
    "embeddedFonts": [["OldStandard-Regular.ttf", "OldStandard-Bold.ttf", "OldStandard-Italic.ttf", "OldStandard-Bold.ttf"]],
    "fontsAlias" : {
        "stdFont": "OldStandard-Regular",
        "stdBold": "OldStandard-Bold",
        "stdItalic": "OldStandard-Italic"
    },
    "styles" :{
        "abstract" : {
            "alignment": "TA_CENTER"
        },
        "normal" : {
            "fontSize": 10
        },
        "blockquote": {
            "alignment": "TA_CENTER",
            "fontName": "stdItalic",
            "spaceAfter": 80
        },
        "title": {
            "fontSize": 24
        },
        "heading1": {
            "fontSize": 16
        },
        "bullet-list": {
            "colWidths": [5]

        },
        "note": {
            "borderPadding": [3,3,3,3],
            "backColor": "white"
        },
        "note-heading": {
            "parent": "heading6",
            "spaceAfter": 0,
            "spaceBefore": 0
        }
    }
}